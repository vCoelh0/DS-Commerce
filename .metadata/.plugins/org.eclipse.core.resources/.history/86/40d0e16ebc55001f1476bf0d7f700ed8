package com.coelhodev.dscommerce.dto;

import java.time.Instant;

public class CustomError {

	private Instant timestamp;
	private String satatus;
	private String error;
	private String path;
	
	
	
	public CustomError(Instant timestamp, String satatus, String error, String path) {
		this.Instant = timestamp;
		this.satatus = satatus;
		this.error = error;
		this.path = path;
	}


	public String getSatatus() {
		return satatus;
	}


	public String getError() {
		return error;
	}


	public String getPath() {
		return path;
	}
	
	
	
	
	
}
